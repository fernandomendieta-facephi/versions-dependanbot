{
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/apps/kargo/.*/kustomization\\.yaml$/"
      ],
      "matchStrings": [
        "name:\\s*kargo[\\s\\S]*?version:\\s*(?<currentValue>.*)"
      ],
      "depNameTemplate": "ghcr.io/akuity/kargo-charts/kargo",
      "datasourceTemplate": "docker"
    }
  ]
}
